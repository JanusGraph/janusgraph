<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <root level="ERROR">
        <appender-ref ref="STDOUT" />
    </root>
    <logger name="org.janusgraph.diskstorage.couchbase" level="DEBUG"/>
    <logger name="org.janusgraph.testutil" level="DEBUG"/>
</configuration>