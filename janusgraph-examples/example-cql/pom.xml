        <artifactId>janusgraph-examples</artifactId>
        <version>0.3.2</version>
        <relativePath>../pom.xml</relativePath>
    </parent>
    <artifactId xmlns="https://maven.apache.org/POM/4.0.0">example-cql</artifactId>
    <packaging xmlns="https://maven.apache.org/POM/4.0.0">pom</packaging>
    <name xmlns="https://maven.apache.org/POM/4.0.0">Example-Cql: Cassandra CQL Storage, Elasticsearch Index</name>
    <url xmlns="https://maven.apache.org/POM/4.0.0">https://janusgraph.org</url>

